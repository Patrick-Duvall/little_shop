<%= form_for [@user, @address] do |f| %>
  <%= f.label :nick_name %>
  <%= f.text_field :nick_name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label  :city %>
  <%= f.text_field :city %>
  <%= f.label  :state %>
  <%= f.text_field :state %>
  <%= f.label  :zip %>
  <%= f.text_field :zip %>
  <%= f.submit %>

<% end %>
